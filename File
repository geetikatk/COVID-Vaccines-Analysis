      "cell_type": "code",
      "source": [
        "#Countries with fully vax <20%\n",
        "q2 = df_vac.groupby(['country'])[['date', 'people_fully_vaccinated_per_hundred']].max()\n",
        "under20 = q2.loc[q2['people_fully_vaccinated_per_hundred']<20,].sort_values(by = 'people_fully_vaccinated_per_hundred', ascending = False)\n",
        "print(under20)\n",
        "print(under20.shape)"
